{
  "name": "vibe-lab-s-s-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/ensure-port-3000.js",
    "dev": "next dev -p 3000",
    "dev:monitor": "node scripts/ensure-port-3000.js && tsx scripts/dev-with-monitoring.ts && next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint src --ext .ts,.tsx",
    "check:pwa": "node scripts/pwa-compliance-check.js",
    "check:architecture": "node scripts/component-architecture-check.js",
    "check:design-system": "npm run check:pwa && npm run check:architecture",
    "fix:design-system": "npm run check:design-system && echo 'ðŸ“š See Design-System-Learning-Log.md for manual fixes'",
    "hooks:install": "git config core.hooksPath .githooks && echo 'âœ… Git hooks installed'",
    "hooks:test": ".githooks/pre-commit",
    "capture:learning": "node scripts/capture-learning.js",
    "test:ai-client": "tsx scripts/test-ai-client.ts",
    "test:microservices": "tsx scripts/test-microservices.ts",
    "test:rate-limit-retry": "tsx scripts/test-rate-limit-retry.ts",
    "test:cost-optimization": "tsx scripts/test-cost-optimization.ts",
    "test:context-manager": "tsx scripts/test-context-manager.ts",
    "test:dias-events": "tsx scripts/test-dias-events.ts",
    "test:integration": "tsx scripts/test-integration.ts",
    "test:full-integration": "tsx scripts/test-full-integration.ts",
    "test:load": "tsx scripts/test-load.ts",
    "test:benchmark": "tsx scripts/benchmark-performance.ts",
    "test:blueprint-parser": "tsx scripts/test-blueprint-parser.ts",
    "test:component-planner": "tsx scripts/test-component-planner.ts",
    "test:code-generator": "tsx scripts/test-code-generator.ts",
    "test:quality-assurance": "tsx scripts/test-quality-assurance.ts",
    "test:logic-monitor": "tsx scripts/test-logic-monitor.ts",
    "test:vibe-lab-monitor": "tsx scripts/test-vibe-lab-monitoring.ts",
    "test:component-system": "tsx scripts/test-component-system.ts",
    "test:performance-optimization": "tsx scripts/test-performance-optimization.ts",
    "test:component-validation": "tsx scripts/test-component-system-validation.ts",
    "test:e2e-enhanced": "tsx scripts/test-e2e-enhanced.ts",
    "test:load-infrastructure": "tsx scripts/test-load-infrastructure.ts",
    "test:load-demo": "tsx scripts/test-load-demo.ts",
    "demo:pipeline": "tsx scripts/demo-component-pipeline.ts",
    "debug:pipeline": "tsx scripts/debug-pipeline.ts",
    "test:all": "npm run test:microservices && npm run test:ai-client && npm run test:rate-limit-retry && npm run test:cost-optimization && npm run test:context-manager && npm run test:dias-events && npm run test:blueprint-parser && npm run test:component-planner && npm run test:code-generator && npm run test:quality-assurance",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npx tsc --noEmit --watch",
    "lint:fix": "npx eslint src --ext .ts,.tsx --fix",
    "quality:check": "npm run type-check && npm run lint && npm run test:ci",
    "quality:fix": "npm run lint:fix && npm audit fix",
    "quality:report": "node scripts/generate-quality-report.js",
    "arch:deps": "npx madge --circular --extensions ts,tsx src/",
    "arch:unused": "npx ts-unused-exports tsconfig.json",
    "arch:analyze": "npm run arch:deps && npm run arch:unused",
    "security:audit": "npx audit-ci --config audit-ci.json",
    "security:check": "npm audit && npx audit-ci",
    "pre-commit": "npm run quality:check",
    "validate": "npm run quality:check && npm run arch:analyze && npm run security:check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@dqbd/tiktoken": "^1.0.21",
    "@octokit/rest": "^22.0.0",
    "@playwright/test": "^1.54.2",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.526.0",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "task-master-ai": "^0.22.0",
    "vis-network": "^10.0.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "madge": "^8.0.0",
    "postcss": "^8.4.31",
    "prisma": "^6.12.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.1",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx tsc --noEmit",
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run quality:check"
    }
  }
}